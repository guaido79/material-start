{
  "name": "material-start",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "start": "concurrent \"npm run tsc:w\" \"live-server --open=app\"",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "typings": "typings",
    "postinstall": "typings install dt~jquery dt~angular dt~angular-animate dt~angular-material --global --save"
  },
  "description": "A Typescript Tutorial project for AngularJS Material Start",
  "repository": "https://github.com/angular/material-start",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^2.2.0",
    "live-server": "^0.9.2"
  },
  "dependencies": {
    "angular": "^1.5.5",
    "angular-animate": "^1.5.8",
    "angular-aria": "^1.5.8",
    "angular-material": "^1.1.0",
    "angular-messages": "^1.5.8",
    "angular-sanitize": "^1.5.8",
    "systemjs": "^0.19.28",
    "typescript": "^1.8.10",
    "typings": "^1.3.2"
  }
}
